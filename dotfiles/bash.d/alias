# These aliases are shared between Bash and Fish

alias ls="lsd "
alias e="nano "

# Git
alias gl="git ls"
alias gll="git ll"
alias glg="git lg"
alias gst="git status"
alias gd="git wdiff"
alias ga="git add"
alias gc="git commit -m "
alias gca="git commit --amend"
alias gco="git checkout "
alias gpu="git push "
alias gpl="git pull "
alias gpf="git push --force vorburger"
alias gsy="git pull --rebase && git push"
# see ../bin/g* for more Git tools

